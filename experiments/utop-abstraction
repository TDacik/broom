*** first you need a valid json file. Get it e.g. by:

scripts/json_dumper tests/sll1.c > j.json

*** run utop based on dune specification

dune utop src/biabduction/

*** paste j.json followed by CTRL-D

*** now you are in utop

open Biabd;;
open Formula;;
open Z3;;
open Abstraction;;
let cfg = [("model", "true"); ("proof", "false")]
let ctx = (mk_context cfg)
let solv = (Solver.mk_solver ctx None)
let z3_names=Z3wrapper.get_sl_functions_z3 ctx;;

try_abstraction_to_lseg ctx solv z3_names form_abstr1 0 2 [1];;
let res=match try_abstraction_to_lseg ctx solv z3_names form_abstr1 0 2 [1] with
| AbstractionApply y -> y;;

print_with_lambda res;;
